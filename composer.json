{
    "name": "gsokol/jsonapi-bundle",
    "homepage": "http://nilportugues.com",
    "type": "library",
    "description": "Symfony 2 & 3 JSON API Transformer Package",
    "keywords": ["sf2", "symfony", "symfony2", "symfony3", "lumen", "json", "api", "jsonapi", "serializer", "transformer", "psr7", "response"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nil Portugués Calderó",
            "email": "contact@nilportugues.com",
            "role": "Project Lead Developer"
        },
        {
            "name": "Grigorii Sokolik",
            "email": "g.sokol99@g-sokol.info",
            "role": "Customizer"
        }
    ],
    "autoload": {
        "psr-4": {
            "NilPortugues\\Symfony\\JsonApiBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "gsokol/json-api": "dev-deploy"
    },
    "require-dev": {
        "symfony/symfony": "2.*",
        "friendsofphp/php-cs-fixer": "~1.9",
        "nilportugues/php_backslasher": "~0.2"
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/GSokol/php-json-api"
        }
    ]
}
